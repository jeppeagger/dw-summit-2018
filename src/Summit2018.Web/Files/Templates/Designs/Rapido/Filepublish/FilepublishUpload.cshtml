<form action="/admin/public/Upload.aspx" method="post" enctype="multipart/form-data">
	<input type="hidden" name="Notify" value="@GetString("FilepublishUploadNotify")">
	<input type="hidden" name="Folder" value="@GetString("FilepublishUploadFolder")">
	<input type="hidden" name="Subject" value="@GetString("FilepublishUploadSubject")">
	<input type="hidden" name="Recipient" value="@GetString("FilepublishUploadRecipient")">
	<input type="hidden" name="RecipientCC" value="@GetString("FilepublishUploadRecipientCC")">
	<input type="hidden" name="RecipientBCC" value="@GetString("FilepublishUploadRecipientBCC")">
	<input type="hidden" name="Text" value="@GetString("FilepublishUploadText")">
	<input type="hidden" name="Confirm" value="@GetString("FilepublishUploadConfirm")">
    <div class="form__field-combi">
        <input type="File" name="File" class="u-full-width">
        <button type="submit" class="btn btn--primary dw-mod">@Translate("Upload")</button>
    </div>
</form>